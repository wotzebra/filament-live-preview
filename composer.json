{
    "name": "wotz/filament-live-preview",
    "description": "A Filament plugin to add a preview screen to your pages. The screen will render your website with the data from Filament, without saving it.",
    "keywords": [
        "wotz",
        "laravel",
        "filament-live-preview"
    ],
    "homepage": "https://github.com/wotz/filament-live-preview",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "illuminate/contracts": "^12.0",
        "livewire/livewire": "^4.0",
        "pboivin/filament-peek": "dev-filament-5 as 4.0",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Wotz\\FilamentLivePreview\\": "src",
            "Wotz\\FilamentLivePreview\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wotz\\FilamentLivePreview\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wotz\\FilamentLivePreview\\Providers\\FilamentLivePreviewServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
